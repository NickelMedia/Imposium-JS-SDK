{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark":[true, "single"],
        "trailing-comma":false,
        "ordered-imports": false,
        "object-literal-sort-keys":false,
        "no-console":false,
        "no-var-requires":false,
        "no-bitwise":false,
        "max-classes-per-file":false
    },
    "rulesDirectory": []
}