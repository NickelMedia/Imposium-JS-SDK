{
    "plugins": [
        "html"
    ],
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 5
    },
    "env": {
        "browser": true,
        "node": true
    },
    "rules":{
        "no-trailing-spaces":"error",
        "no-unused-vars":["error", {"vars":"local"}],
        "quotes":["error", "single"],
        "no-console":"off",
        "camelcase":"error",
        "no-empty-function"
    },
    "globals":{
        "Imposium": true
    }
}