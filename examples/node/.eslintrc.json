{
    "extends": [
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node": true,
        "browser": false
    },
    "rules":{
        "no-trailing-spaces":"error",
        "no-unused-vars":["error", {"vars":"local"}],
        "quotes":["error", "single"],
        "no-console":"off",
        "camelcase":"error",
        "no-empty-function":"error"
    },
    "globals":{
        "Imposium": true
    }
}
