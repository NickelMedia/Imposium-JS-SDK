{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "rules": {
        "trailing-comma": false,
        "ordered-imports": false,
        "object-literal-sort-keys": false,
        "no-console": false,
        "no-var-requires": false,
        "no-bitwise": false,
        "max-classes-per-file": false,
        "variable-name": [
            true,
            "check-format"
        ],
        "quotemark": [
            true,
            "single"
        ]
    }
}
